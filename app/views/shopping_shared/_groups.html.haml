.content#groups
  %ul
    - for group in current_distributor.groups
      %li
        %h4= group.name 
        %ul
          - for sibling in group.enterprises.except(current_distributor)
            %li
              %a{"data-reveal-id" => "sibling_details_#{sibling.id}", "data-reveal" => ""}
                = sibling.name 
